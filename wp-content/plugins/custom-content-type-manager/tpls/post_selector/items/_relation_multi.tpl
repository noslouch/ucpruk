<tr id="cctm_tr_multi_select_[+ID+]">
	<td>
		<input type="checkbox" value="[+ID+]" id="cctm_multi_select_[+ID+]"/>
		<label for="cctm_multi_select_[+ID+]">
			<img class="cctm_tiny_thumb" src="[+thumbnail_url+]" height="30" width="30" alt=""/>
		</label>
	</td> 
	<td>[+post_title+] ([+ID+])</td>
	<td>[+post_type+]</td>
	<td>[+post_date+]</td>
	<td><a href="[+guid+]" target="_new">[+preview+]</a></td>
</tr>